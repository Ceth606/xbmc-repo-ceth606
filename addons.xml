<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="repository.ceth606" name="ceth606 Add-on Repository" version="1.0.0" provider-name="Ceth606">
        <extension point="xbmc.addon.repository" name="ceth606 Add-on Repository">
                <info compressed="false">http://xbmc-repo-ceth606.googlecode.com/svn/trunk/addons.xml</info>
                <checksum>http://xbmc-repo-ceth606.googlecode.com/svn/trunk/addons.xml.md5</checksum>
                <datadir zip="true">http://xbmc-repo-ceth606.googlecode.com/svn/trunk/</datadir>
        </extension>
        <extension point="xbmc.addon.metadata">
                <summary>Install Add-ons from ceth606 Add-on Repository</summary>
                <description>XBMC video plugin</description>
                <disclaimer></disclaimer>
                <platform>all</platform>
        </extension>
</addon>

<addon id="plugin.video.frmovies"
       name="frmovies"
       version="1.0.2"
       provider-name="Ceth606">
  <requires>
    <import addon="xbmc.python" version="2.0"/>
    <import addon="script.module.urlresolver" version="1.0.0"/>
    <import addon="script.module.metahandler" version="1.0.2"/>
    <import addon="script.module.t0mm0.common" version="1.0.0"/>
    <import addon="script.common.plugin.cache" version="0.9.1"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en"></summary>
    <description lang="en">Visit www.xbmchub.com for support.</description>
    <platform>all</platform>
  </extension>
</addon>
</addons>
